program ArithmeticOperations;
var
    a, b, sum, difference, product, quotient: integer;
begin
    a := 15;
    b := 5;
    sum := a + b;
    difference := a - b;
    product := a * b;
    quotient := a div b;
    writeln('Sum: ', sum);
    writeln('Difference: ', difference);
    writeln('Product: ', product);
    writeln('Quotient: ', quotient);
end.
